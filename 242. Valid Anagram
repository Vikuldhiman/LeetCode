class Solution {
    public boolean isAnagram(String s, String t) {
        int count=0;
        char temp1[] = s.toCharArray();
        char temp2[] = t.toCharArray();
        Arrays.sort(temp1);
        Arrays.sort(temp2);
        for(int i=0;i<temp1.length;i++){
                if(temp1[i]==temp2[i]){
                    count++;
            }
        }
        if((count==temp1.length)&&(count==temp2.length))
        {
            return true;
        }else{
            return false;
        }
    }
}
